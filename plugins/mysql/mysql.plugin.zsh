# commands to control local mysql-server installation
# paths are for osx installation : path /usr/local/mysql

alias mysql-start="sudo sh /usr/local/mysql/support-files/mysql.server start"
alias mysql-stop="sudo sh /usr/local/mysql/support-files/mysql.server stop"
alias mysql-stats="sudo sh /usr/local/mysql/support-files/mysql.server stats"
alias mysql-restart="sudo sh /usr/local/mysql/support-files/mysql.server restart"
